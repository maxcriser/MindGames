<?xml version="1.0" encoding="utf-8"?>
<resources>

    <item name="match_parent" type="dimen">-1</item>
    <item name="wrap_content" type="dimen">-2</item>

    <declare-styleable name="mg_toolbar">
        <attr name="title" format="string"/>
        <attr name="background_color" format="color"/>
        <attr name="action" format="integer">
            <flag name="menu" value="1"/>
            <flag name="back" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="mg_drawer_button">
        <attr name="item_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="MaterialSquareLoading">
        <attr name="innerColor" format="color"/>
        <attr name="outerColor" format="color"/>
        <attr name="rotationInnerDuration" format="integer"/>
        <attr name="rotationOuterDuration" format="integer"/>
        <attr name="outerRadius" format="dimension"/>
        <attr name="innerRadius" format="dimension"/>
    </declare-styleable>
</resources>